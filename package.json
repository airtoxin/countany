{
  "name": "countany",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev --port 3001",
    "dev:codegen": "graphql-codegen --watch",
    "codegen": "graphql-codegen",
    "build": "run-s codegen build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "run-s test:*",
    "test:lint": "next lint",
    "test:unit": "dotenv -e .env.test.local -- jest"
  },
  "dependencies": {
    "graphql": "16.3.0",
    "hashids": "2.2.10",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-query": "3.38.0",
    "undici": "5.8.2",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/near-operation-file-preset": "2.2.9",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-query": "3.5.9",
    "@graphql-codegen/typescript-resolvers": "2.6.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.25",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "dotenv-cli": "5.1.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.6.3"
  }
}
